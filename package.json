{
  "name": "apprun-starter",
  "version": "1.0.0",
  "description": "AppRun Application Starter Template",
  "main": "index.html",
  "scripts": {
    "test": "jest --watch",
    "start": "run-p start:*",
    "start:dev": "sirv public -s -D",
    "start:pages": "apprun-site build -w -V",
    "build": "run-s build:*",
    "build:pages": "apprun-site build",
    "build:js": "webpack -p --mode production",
    "build:esm": "run-s build:esm:*",
    "build:esm:ts": "tsc -p src",
    "build:esm:fix": "apprun-site fix-esm --source public/esm"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "css-loader": "^2.1.1",
    "extract-loader": "^3.1.0",
    "file-loader": "^4.0.0",
    "glob": "^7.1.4",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "sirv-cli": "^0.4.3",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "dependencies": {
    "apprun": "^2.20.6",
    "apprun-site": "^0.5.0",
    "material-components-web": "^2.3.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  }
}
